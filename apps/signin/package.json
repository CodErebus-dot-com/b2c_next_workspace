{
  "name": "signin",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "rimraf .next && cross-env NEXT_PRIVATE_LOCAL_WEBPACK=true next dev",
    "process-tw": "rimraf tw-styles.js && node src/scripts/tailwind.js",
    "build": "node src/scripts/build.js",
    "clean": "rimraf .turbo .next",
