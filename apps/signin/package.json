  "dependencies": {
    "@acme/ui": "workspace:*",
    "@module-federation/nextjs-mf": "8.1.10",
    "@module-federation/utilities": "3.0.5",
    "@nextui-org/react": "2.2.9",
    "critters": "^0.0.19",
    "markdown-to-jsx": "7.4.1",
    "next": "13.5.6",
    "react": "^18.2",
    "react-dom": "^18.2",
    "styled-jsx": "^5.1.2",
    "webpack": "^5.90.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.1",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "29.5.12",
    "@types/node": "^18.0.0",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "autoprefixer": "^10.4.12",
    "cross-env": "7.0.3",
    "eslint": "^8",
    "eslint-config-next": "13.5.5",
    "jest": "29.7.0",
    "jest-mock-console": "2.0.0",
    "postcss": "^8.4.18",
    "rimraf": "^4.4.0",
    "sharp": "^0.33.2",
    "tailwindcss": "^3.2.1",
    "typescript": "5.1.6"
